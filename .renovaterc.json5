{
  extends: [
    'config:recommended',
    ':dependencyDashboard',
    ':disableRateLimiting',
    'docker:enableMajor',
    'docker:pinDigests',
    ':automergeDigest',
    ':enablePreCommit',
    ':semanticCommits',
  ],
  gitAuthor: 'repo-jeeves <106431701+repo-jeeves[bot]@users.noreply.github.com>',
  timezone: 'America/Toronto',
  dependencyDashboardTitle: 'Renovate Dashboard',
  suppressNotifications: [
    'prIgnoreNotification',
  ],
  commitBodyTable: true,
  commitMessageTopic: '{{depName}}',
  commitMessageExtra: 'to {{newVersion}}',
  commitMessageSuffix: '',
  customManagers: [],
  packageRules: [
    {
      matchManagers: [
        'github-actions',
      ],
      automerge: true,
      automergeType: 'branch',
      ignoreTests: true,
      matchUpdateTypes: [
        'minor',
        'patch',
      ],
    },
    {
      matchUpdateTypes: [
        'major',
      ],
      labels: [
        'type/major',
      ],
    },
    {
      matchUpdateTypes: [
        'minor',
      ],
      labels: [
        'type/minor',
      ],
    },
    {
      matchUpdateTypes: [
        'patch',
      ],
      labels: [
        'type/patch',
      ],
    },
    {
      matchDatasources: [
        'docker',
      ],
      addLabels: [
        'docker',
      ],
    },
    {
      matchManagers: [
        'github-actions',
      ],
      addLabels: [
        'ci',
      ],
    },
    {
      matchDatasources: [
        'cargo',
      ],
      addLabels: [
        'dependency',
      ],
    },
    {
      matchDatasources: [
        'cargo',
      ],
      commitMessageTopic: 'Crate {{depName}}',
    },
    {
      matchDatasources: [
        'docker',
      ],
      commitMessageTopic: 'Docker image {{depName}}',
    },
    {
      matchUpdateTypes: [
        'major',
      ],
      semanticCommitType: 'feat!',
    },
    {
      matchUpdateTypes: [
        'minor',
      ],
      semanticCommitType: 'feat',
    },
    {
      matchUpdateTypes: [
        'patch',
      ],
      semanticCommitType: 'fix',
    },
    {
      matchDatasources: [
        'docker',
      ],
      semanticCommitScope: 'deps',
    },
    {
      matchDatasources: [
        'cargo',
      ],
      semanticCommitScope: 'deps',
    },
    {
      matchManagers: [
        'github-actions',
      ],
      semanticCommitScope: 'deps',
    },
  ],
}
